{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Launch Program",
          "env": {
          },
          "skipFiles": [
              "<node_internals>/**"
          ],
          "outputCapture": "std",
          "program": "${workspaceFolder}\\dist\\index.js",
          "preLaunchTask": "tsc: build - tsconfig.json",
          "outFiles": [
              "${workspaceFolder}/dist/**/*.js"
          ]
      }
  ]
}